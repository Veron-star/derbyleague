{
  "name": "derbyleague",
  "version": "1.0.0",
  "description": "roller derby",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'frontend/*'\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run start",
    "seed": "node database/seed/index.js",
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Veron-star/derbyleague.git"
  },
  "author": "veronica",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Veron-star/derbyleague/issues"
  },
  "homepage": "https://github.com/Veron-star/derbyleague#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "react-smooth-scroll": "^0.1.0",
    "serve-favicon": "^2.5.0"
  }
}
