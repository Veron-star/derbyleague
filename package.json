{
  "name": "derbyleague",
  "version": "1.0.0",
  "description": "roller derby",
  "main": "backend/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'frontend/*'\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run start",
    "seed": "node database/seed/index.js",
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Veron-star/derbyleague.git"
  },
  "author": "veronica",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Veron-star/derbyleague/issues"
  },
  "homepage": "https://github.com/Veron-star/derbyleague#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^5.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.5.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mailchimp-api-v3": "^1.14.0",
    "mailgun-js": "^0.22.0",
    "mobile-detect": "^1.4.4",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "mongoose-slug-generator": "^1.0.4",
    "mongoose-unique-validator": "^2.0.3",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "nodemailer": "^6.4.14",
    "nodemailer-mailgun-transport": "^2.0.1",
    "nodemon": "^2.0.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "sass-loader": "^10.0.5",
    "serve-favicon": "^2.5.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-slug": "^2.3.2",
    "validator": "^13.1.17"
  },
  "_moduleAliases": {
    "@config": "config",
    "@controllers": "controllers",
    "@middleware": "middleware",
    "@models": "models",
    "@routes": "routes",
    "@util": "util",
    "@validations": "validations"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.3.0",
    "webpack-pwa-manifest": "^4.2.0"
  }
}
